@model MTDTextFieldTagsModel


@if (Model.IconTrailing != null)
{
    if (Model.IconTrailingString != null)
    {
        <i class="mdc-text-field__icon mdc-text-field__icon--leading" @Model.IconTrailing
           tabindex="0" role="button" onclick="@Model.IconTrailingString"></i>
    }
    else
    {
        <i class="mdc-text-field__icon mdc-text-field__icon--leading @Model.IconTrailing"></i>
    }
}