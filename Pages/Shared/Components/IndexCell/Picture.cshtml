@model Models.Index.CellModelView

<div>
    @if (@Model.MtdStoreStack.MtdStoreStackFile != null)
    {
        var base64 = Convert.ToBase64String(Model.MtdStoreStack.MtdStoreStackFile.Register);
        var imgSrc = String.Format("data:{0};base64,{1}", Model.MtdStoreStack.MtdStoreStackFile.FileType, base64);
        <img src="@imgSrc" style="max-height:48px;" />
    }
    else
    {
        <div class="mtd-index-rows-image"></div>
    }
</div>