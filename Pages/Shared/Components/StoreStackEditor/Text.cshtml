@model MtdStoreStack

@await Component.InvokeAsync(typeof(MTDTextField), new MTDTextFieldTags
{
    Id = Model.MtdFormPartFieldNavigation.Id,
    Name = Model.MtdFormPartFieldNavigation.Id,
    Value = Model.MtdStoreStackText.Register,
    Label = Model.MtdFormPartFieldNavigation.Name,
    Required = Model.MtdFormPartFieldNavigation.Required == 1,
    MTDTexFieldView = MTDTextFieldViews.Outlined,
    MaxLength = 150,
})




